# This file is for unifying the coding style for different editors and IDEs
# See editorconfig.org

root = true

[*]
indent_style = space
end_of_line = lf
charset = utf-8
trim_trailing_whitespace = true
insert_final_newline = true

[*.java]
indent_size = 4

[*.tf]
indent_size = 2

[*.xml]
indent_size = 4

[*.yml]
indent_size = 2
